      {
        "clash_mode": "direct",
        "server": "ali-dns"
      },
      {
        "clash_mode": "global",
        "server": "cf-dns"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "query_type": ["AAAA"],
        "action": "predefined",
        "rcode": "NOERROR",
        "answer": []
      },
      {
        "rule_set": "manual-dns-proxy",
        "server": "cf-dns"
      },
      {
        "rule_set": "manual-dns-direct",
        "server": "ali-dns"
      },
      {
        "rule_set": "geosite-cn",
        "server": "ali-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "server": "cf-dns",
        "client_subnet": "114.114.114.114/24"
      }
    ],
    "final": "cf-dns",
    "strategy": "prefer_ipv4"
  },
